	<build>
		<finalName>SIGEM_RegistroPresencialWeb</finalName>
		<resources>
			<resource>
				<filtering>true</filtering>
				<directory>src/main/resources</directory>
				<excludes>
					<exclude>**/*.jpg</exclude>
					<exclude>**/*.gif</exclude>
				</excludes>
			</resource>
			<resource>
				<filtering>true</filtering>
				<directory>${unpack.config.directory}</directory>
				<excludes>
					<exclude>**/*.jpg</exclude>
					<exclude>**/*.gif</exclude>
					<exclude>**/log4j.xml</exclude>
				</excludes>
			</resource>
			<resource>
				<filtering>false</filtering>
				<directory>${unpack.config.directory}</directory>
				<includes>
					<include>**/*.jpg</include>
					<include>**/*.gif</include>
					<include>**/log4j.xml</include>
				</includes>
			</resource>
		</resources>
		<filters>
