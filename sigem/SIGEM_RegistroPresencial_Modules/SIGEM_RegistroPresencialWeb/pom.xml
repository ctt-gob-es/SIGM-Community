								</artifactItem>
							</artifactItems>
							<outputDirectory>
								${unpack.config.directory}
							</outputDirectory>
						</configuration>
					</execution>
					<execution>
						<id>unpack-web-resources</id>
						<phase>generate-resources</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>es.ieci.tecdoc.isicres</groupId>
