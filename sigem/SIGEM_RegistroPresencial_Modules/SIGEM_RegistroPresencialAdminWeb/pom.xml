	<build>
		<finalName>SIGEM_RegistroPresencialAdminWeb</finalName>

		<resources>
			<resource>
				<filtering>true</filtering>
				<directory>${basedir}/src/main/resources</directory>
			</resource>
			<resource>
				<filtering>true</filtering>
				<directory>${unpack.config.directory}</directory>
				<excludes>
					<exclude>resources/*.properties</exclude>
					<exclude>*.properties</exclude>
					<exclude>SIGEM_RegistroPresencial/Isicres/*.xml</exclude>
					<exclude>SIGEM_RegistroPresencial/Isicres/*.properties</exclude>
				</excludes>
			</resource>
			<resource>
				<filtering>true</filtering>
				<directory>${unpack.config.directory}/SIGEM_RegistroPresencial/Isicres</directory>
				<targetPath>${unpack.config.directory}/SIGEM_RegistroPresencial</targetPath>
				<excludes>
					<exclude>**/log4j.xml</exclude>
				</excludes>
			</resource>
		</resources>


		<filters>
			<filter>src/main/filters/Common.properties</filter>
		</filters>

		<plugins>

			<!-- plugin de overlay correcto con dependencias -->
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.1-alpha-1</version>
				<configuration>
					<warSourceExcludes>${source.exclude}</warSourceExcludes>
					<!-- se configura esto para que haga el overlay de librerias correcto con ayuda del plugin de appfuse -->
					<dependentWarExcludes>WEB-INF/lib/*, WEB-INF/classes/Isicres/*, WEB-INF/classes/ISicresAdminWeb/*,resources/**</dependentWarExcludes>
					<webResources>
						<resource>
							<directory>
								src/main/webapp/WEB-INF
							</directory>
							<targetPath>WEB-INF</targetPath>
							<filtering>true</filtering>
						</resource>
						 <resource>
					        <directory>src/main/webapp/resources</directory>
					        <filtering>true</filtering>
					        <targetPath>${targetPathStatic}</targetPath>
					        <excludes>
					          <exclude>**/*.css</exclude>
					          <exclude>**/*.js</exclude>
					           <exclude>**/*.png</exclude>
					           <exclude>**/*.jpg</exclude>
					           <exclude>**/*.gif</exclude>
					           <exclude>**/*.pdf</exclude>
					        </excludes>
					      </resource>
					</webResources>
				
					<archive>
			            <manifest>
			              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
			            </manifest>
			            <manifestEntries>
			              <Implementation-Build>${buildNumber}</Implementation-Build>
			              <Weblogic-Application-Version>${sigm.pom.version}</Weblogic-Application-Version>
			            </manifestEntries>
			          </archive>
					<packagingExcludes>
						WEB-INF/lib/activation*.jar,
						WEB-INF/lib/bcmail-jdk15*.jar,
						WEB-INF/lib/bcprov-jdk15*.jar,
						WEB-INF/lib/commons-lang*.jar,
						WEB-INF/lib/commons-logging*.jar,
						WEB-INF/lib/commons-net*.jar,
						WEB-INF/lib/dom4j*.jar,
						WEB-INF/lib/icu4j*.jar,
						WEB-INF/lib/jaxen*.jar,
						WEB-INF/lib/jcommon*.jar,
						WEB-INF/lib/jdom*.jar,
						WEB-INF/lib/jdtcore*.jar,
						WEB-INF/lib/jfreechart*.jar,
						WEB-INF/lib/jstl*.jar,
						WEB-INF/lib/log4j*.jar,
						WEB-INF/lib/mail*.jar,
						WEB-INF/lib/oro*.jar,
						WEB-INF/lib/saaj-api*.jar,
						WEB-INF/lib/saaj-impl*.jar,
						WEB-INF/lib/serializer*.jar,
						WEB-INF/lib/slf4j-api*.jar,
						WEB-INF/lib/slf4j-log4j12*.jar,
						WEB-INF/lib/standard*.jar,
						WEB-INF/lib/wss4j*.jar,
						WEB-INF/lib/xalan*.jar,
						WEB-INF/lib/xercesImpl*.jar,
						WEB-INF/lib/xml-apis*.jar,
						WEB-INF/lib/xmlParserAPIs*.jar,
						WEB-INF/lib/xom*.jar,
						WEB-INF/lib/alfresco-repository*.jar,
						WEB-INF/lib/alfresco-web-service-client*.jar,
						WEB-INF/lib/sigem_clasesBase*.jar
					</packagingExcludes>
				</configuration>
			</plugin>

			<!-- Ficheros de configuracion de ISicres e ISicresAdmin -->
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>dependency-maven-plugin</artifactId>
				<executions>
					<execution>
						<id>unpack-config-resources</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>es.ieci.tecdoc.isicres</groupId>
									<artifactId>ISicres-resources-config</artifactId>
									<version>${isicres.version}</version>
									<type>zip</type>
								</artifactItem>
							</artifactItems>
							<outputDirectory>
								${unpack.config.directory}
							</outputDirectory>
						</configuration>
					</execution>
					<execution>
						<id>unpack-configurable-resources</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>es.ieci.tecdoc.isicres</groupId>
									<artifactId>ISicres-resources-configurable</artifactId>
									<version>${isicres.version}</version>
									<type>zip</type>
								</artifactItem>
							</artifactItems>
							<outputDirectory>
								${unpack.config.directory}/SIGEM_RegistroPresencial
							</outputDirectory>
						</configuration>
					</execution>
					<execution>
						<id>unpack-config-admin-resources</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>es.ieci.tecdoc.isicres.admin</groupId>
									<artifactId>ISicresAdmin-resources-config</artifactId>
									<version>${isicres.version}</version>
									<type>zip</type>
								</artifactItem>
							</artifactItems>
							<outputDirectory>
								${unpack.config.directory}
							</outputDirectory>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-resources-plugin</artifactId>
				<version>2.5</version>
				<executions>
					<execution>
						<id>copy-resources</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>copy-resources</goal>
						</goals>
						<configuration>
							<outputDirectory>${basedir}/target/resources-filtered</outputDirectory>
							<filters>
								<filter>${filter.generic.system}</filter>
							</filters>
							<useBuildFilters>false</useBuildFilters>
							<escapeWindowsPaths>false</escapeWindowsPaths>
							<escapeString>false</escapeString>
							<resources>
								<resource>
									<directory>${basedir}/src/main/filters/system</directory>
									<includes>
										<include>Unix.properties</include>
										<include>Windows.properties</include>
										<include>msssi.properties</include>
									</includes>
	      							<filtering>true</filtering>
								</resource>
							</resources>
						</configuration>
					</execution>
				</executions>
			</plugin>

		<!-- plugin de overlay correcto con dependencias -->
		 	<plugin>
				<groupId>org.appfuse</groupId>
				<artifactId>maven-warpath-plugin</artifactId>
				 <version>2.0.2</version>
				 <extensions>true</extensions>
			      <executions>
			        <execution>
			          <goals>
			            <goal>add-classes</goal>
			          </goals>
			        </execution>
			      </executions>
			</plugin>

		</plugins>
	</build>

