--><entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm/orm_1_0.xsd">

	<named-query name="sign.signsByValidDateCount">
		<query><![CDATA[select count(sign)
						from PfSignsDTO sign
						where sign.fValid between current_date() and :fecha
						and sign.ctype = 'BLOB']]></query>
	</named-query>
	<named-query name="sign.signsByValidDate">
		<query><![CDATA[select sign
						from PfSignsDTO sign
						where sign.fValid between current_date() and :fecha
						and sign.ctype = 'BLOB'
						order by sign.fValid asc, sign.pfDocument.pfRequest.dsubject asc, sign.pfDocument.dname asc]]></query>
	</named-query>

</entity-mappings>
